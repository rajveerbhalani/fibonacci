nterms = int(input("How many terms?"))
n1,n2 = 0,1
cout = 0
if nterms<=0:
    print("please enter a possitive number")

elif nterms ==1 :
     print("Fibonnaci sequence  upto ",nterms,".")
     print(n1)

else:
    print("Fibonnaci sequence:")
    while cout<nterms
    print(n1)
    nth =n1+n2
    n1=n2
    n2=nth
    cout +=1   
